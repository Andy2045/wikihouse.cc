<div id="enquiries-container">
  <div class="container" id="faq">
    <%= render "header" %>
    <%= title params[:id].gsub('-','_').humanize %>
    <%= simple_form_for Enquiry.new do |f| %>
      <%= render "enquiries/partials/#{params[:id].gsub('-','_')}.html.erb", f: f %>
    <% end %>
  </div>
</div>
